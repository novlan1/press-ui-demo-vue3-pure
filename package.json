    "qrcode.vue": "^3.4.1",
    "swiper-next": "^0.1.1",
    "t-comm": "^1.6.10",
    "tinycolor2": "^1.6.0",
    "vite-babel-plugin": "^0.0.2",
    "vue": "^3.4.29",
    "vue-cropper": "^1.1.4",
    "vue-qrcode": "^2.2.2",
    "vue-router": "^4.3.3",
